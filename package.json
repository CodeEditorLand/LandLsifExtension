{
	"name": "lsif-browser",
	"version": "0.0.1",
	"private": false,
	"description": "Extension to browse LSIF databases",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandLsifExtension#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandLsifExtension/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandLsifExtension.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./client/out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"clean": "tsc -b --clean",
		"compile": "tsc -b",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "lsif.openDatabase",
				"title": "Open LSIF Database"
			}
		],
		"configuration": {
			"properties": {
				"lsif.trace.server": {
					"default": "off",
					"description": "Traces the communication between VS Code and the language server.",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"scope": "window",
					"type": "string"
				}
			},
			"title": "Index Format configuration",
			"type": "object"
		}
	},
	"activationEvents": [
		"onFileSystem:lsif",
		"onCommand:lsif.openDatabase"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.7"
	},
	"publishConfig": {
		"access": "public"
	},
	"enabledApiProposals": [
		"documentFiltersExclusive"
	]
}
