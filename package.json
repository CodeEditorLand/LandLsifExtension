{
  "name": "lsif-browser",
  "version": "0.6.0",
  "description": "Extension to browse LSIF databases",
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-lsif-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/vscode-lsif-extension.git"
  },
  "license": "MIT",
  "author": "Microsoft Corporation",
  "publisher": "ms-vscode",
  "main": "./client/out/extension",
  "scripts": {
    "clean": "tsc -b --clean",
    "compile": "tsc -b",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -b -w"
  },
  "contributes": {
    "commands": [
      {
        "command": "lsif.openDatabase",
        "title": "Open LSIF Database"
      }
    ],
    "configuration": {
      "properties": {
        "lsif.trace.server": {
          "default": "off",
          "description": "Traces the communication between VS Code and the language server.",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "scope": "window",
          "type": "string"
        }
      },
      "title": "Index Format configuration",
      "type": "object"
    }
  },
  "activationEvents": [
    "onFileSystem:lsif",
    "onCommand:lsif.openDatabase"
  ],
  "devDependencies": {
    "@types/node": "18.14.6",
    "typescript": "4.9.5"
  },
  "enabledApiProposals": [
    "documentFiltersExclusive"
  ]
}
